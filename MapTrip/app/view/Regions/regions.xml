<Page loaded="onPageLoaded">
    <ListView id="saitList" items="{{ localImagesArray }}" itemTap="regionItemTapped" width="strech" height="strech">
        <ListView.itemTemplate>
            <StackLayout>
                <Label text="{{ Name }}" />
                <Image src="{{ Picture }}" />
            </StackLayout>
        </ListView.itemTemplate>
    </ListView>
</Page>
